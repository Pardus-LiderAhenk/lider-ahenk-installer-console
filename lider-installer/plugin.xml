<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="tr.org.pardus.mys.liderconsole.commands.category"
            name="%category.name">
      </category>         
      <command
            categoryId="tr.org.pardus.mys.liderconsole.commands.category"
            id="tr.org.liderahenk.installer.commands.LiderInstallationHandler"            
            name="%command.name">
      </command>
   </extension>
   
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="tr.org.liderahenk.installer.lider.handler.LiderInstallationHandler"
            commandId="tr.org.liderahenk.installer.commands.LiderInstallationHandler">
      </handler>
   </extension>
   
     <extension
           point="tr.org.liderahenk.installer.rcp.tr.org.liderahenk.installer.rcp.mainview">
        <viewButton
              commandId="tr.org.liderahenk.installer.commands.LiderInstallationHandler"
              icon="lider-button.png"
              id="tr.org.liderahenk.installer.commands.LiderInstallationHandler">
        </viewButton>
     </extension>
   
</plugin>
